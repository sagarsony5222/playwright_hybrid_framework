<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Suite" verbose="1" parallel="methods" thread-count="4">
    <listeners>
        <listener class-name="listeners.TestListener"/>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chromium"/>

    <test name="Parallel Web Tests" preserve-order="false" parallel="methods" thread-count="4">
        <classes>
            <class name="tests.web.LoginTest"/>
            <class name="tests.web.HomeTest"/>
        </classes>
    </test>
</suite>
